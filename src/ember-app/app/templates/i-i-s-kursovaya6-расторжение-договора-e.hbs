<h3 class="ui header">{{t "forms.i-i-s-kursovaya6-расторжение-договора-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-kursovaya6-расторжение-договора-e.дата-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.дата
      class=(if (v-get validationModel "дата" "isInvalid") "error")
      data-test-i-i-s-kursovaya6-расторжение-договора-e-дата=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "дата" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номер
      class=(if (v-get validationModel "номер" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya6-расторжение-договора-e.номер-caption")
      data-test-i-i-s-kursovaya6-расторжение-договора-e-номер=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номер" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.клиент.id
      class=(if (v-get validationModel.клиент "id" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya6-расторжение-договора-e.клиент.id-caption")
      data-test-i-i-s-kursovaya6-расторжение-договора-e-клиент-id=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.клиент "id" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.клиент.фамилия
      class=(if (v-get validationModel.клиент "фамилия" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya6-расторжение-договора-e.клиент.фамилия-caption")
      data-test-i-i-s-kursovaya6-расторжение-договора-e-клиент-фамилия=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.клиент "фамилия" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.клиент.имя
      class=(if (v-get validationModel.клиент "имя" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya6-расторжение-договора-e.клиент.имя-caption")
      data-test-i-i-s-kursovaya6-расторжение-договора-e-клиент-имя=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.клиент "имя" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.клиент.отчество
      class=(if (v-get validationModel.клиент "отчество" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya6-расторжение-договора-e.клиент.отчество-caption")
      data-test-i-i-s-kursovaya6-расторжение-договора-e-клиент-отчество=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.клиент "отчество" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.директорОтделаПоРаботеСКлиентами.фамилия
      class=(if (v-get validationModel.директорОтделаПоРаботеСКлиентами "фамилия" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya6-расторжение-договора-e.директорОтделаПоРаботеСКлиентами.фамилия-caption")
      data-test-i-i-s-kursovaya6-расторжение-договора-e-директорОтделаПоРаботеСКлиентами-фамилия=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.директорОтделаПоРаботеСКлиентами "фамилия" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.директорОтделаПоРаботеСКлиентами.имя
      class=(if (v-get validationModel.директорОтделаПоРаботеСКлиентами "имя" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya6-расторжение-договора-e.директорОтделаПоРаботеСКлиентами.имя-caption")
      data-test-i-i-s-kursovaya6-расторжение-договора-e-директорОтделаПоРаботеСКлиентами-имя=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.директорОтделаПоРаботеСКлиентами "имя" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=true
      value=model.директорОтделаПоРаботеСКлиентами.отчество
      class=(if (v-get validationModel.директорОтделаПоРаботеСКлиентами "отчество" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya6-расторжение-договора-e.директорОтделаПоРаботеСКлиентами.отчество-caption")
      data-test-i-i-s-kursovaya6-расторжение-договора-e-директорОтделаПоРаботеСКлиентами-отчество=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.директорОтделаПоРаботеСКлиентами "отчество" "message")}}
  </div>
</form>
